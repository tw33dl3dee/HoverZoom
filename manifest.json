{
    "name": "Hover Zoom",
    "version": "3.1",
    "description": "Enlarge thumbnails on mouse over. Works on many sites (Facebook, Twitter, Google™, Flickr, Reddit, Amazon, etc).",
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"minimum_chrome_version": "6.0.472.63",
	"background_page": "background.html",
	"options_page": "options.html",
	"page_action": {
		"default_icon": "images/icon19.png",
		"default_popup": "popup.html",
		"default_title": "Hover Zoom"
	},	
	"permissions": [
		"tabs",
		"history",
		"http://*/*"
	],
	"content_scripts": [
        {
            "js": ["hoverzoom.js"],
			"matches": ["<all_urls>"],
